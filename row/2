#include<stdio.h>
int main(void)
{
    unsigned int a;
    unsigned int b;
    double x;
    char y;
    scanf("%d*%d ",&a,&b);
    y=getchar();
    while(y=='+'){
        x=a+b;
    }
    while(y=='-'){
        x=a-b;
    }
    while(y=='*'){
        x=a*b;
    }
    while(y=='/'){
        x=a/b;
    }
    printf("%.1f\n",x);
    return 0;
}
